<h1>Listing event_lines</h1>

<table>
  <tr>
  </tr>
  
<% for event_line in @event_lines %>
  <tr>
    <td><%= link_to 'Show', event_line_path(event_line) %></td>
    <td><%= link_to 'Edit', edit_event_line_path(event_line) %></td>
    <td><%= link_to 'Destroy', event_line_path(event_line), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event_line', new_event_line_path %>